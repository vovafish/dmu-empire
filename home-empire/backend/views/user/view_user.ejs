<%- include('../index_top') %> <%- include('../header') %> <%-
include('../sidebar') %>

<div class="page-wrapper">
  <!--page-content-wrapper-->
  <div class="page-content-wrapper">
    <div class="page-content">
      <!--breadcrumb-->
      <div class="page-breadcrumb d-none d-md-flex align-items-center mb-3">
        <div class="breadcrumb-title pr-3"><%= section_title %></div>
        <div class="pl-3">
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
              <li class="breadcrumb-item">
                <a href="javascript:;"><i class="bx bx-home-alt"></i></a>
              </li>
              <li class="breadcrumb-item active" aria-current="page">
                <%= page_title %>
              </li>
            </ol>
          </nav>
        </div>
      </div>
      <!--end breadcrumb-->
      <div class="card">
        <div class="card-body">
          <div class="card-title">
            <h4 class="mb-0"><%= view_title %></h4>
          </div>
          <hr />
          <div class="table-responsive">
            <table
              id="example"
              class="table table-striped table-bordered"
              style="width: 100%"
            >
              <thead>
                <tr>
                  <th>S. No</th>
                  <th>First Name</th>
                  <th>Last Name</th>
                  <th>Email</th>
                  <th>Phone</th>
                  
                  <th>Date of Birth</th>
                  <th>Actions</th>
                </tr>
              </thead>
              <tbody>
                <% fetch_users.map((item,index)=>{ %>
                <tr>
                  <td><%= index+1 %></td>

                  <td><%= item.first_name %></td>
                  <td><%= item.last_name %></td>
                  <td><%= item.email %></td>
                  <td><%= item.phone_number %></td>
                  <td><%= item.dob %></td>
                  <td>
                    <a
                      href="<%= `${update_link}/${item._id}` %>"
                      class="btn btn-warning"
                      ><i class="bx bx-edit"></i
                    ></a>
                    <a
                      href="<%= `${delete_link}/${item._id}` %>"
                      class="btn btn-danger"
                      ><i class="bx bx-trash"></i
                    ></a>
                  </td>
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--end page-content-wrapper-->
</div>
<script src="assets/plugins/datatable/js/jquery.dataTables.min.js"></script>
<script>
  $(document).ready(function () {
    //Default data table
    $("#example").DataTable();
  });
</script>

<%- include('../index_bottom') %>
